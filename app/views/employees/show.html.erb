<h1>Employee Info</h1>
<h3>Name: <%= @employee.name %></h3>
<p>Alias: <%= @employee.alias %></p>
<p>Title: <%= @employee.title %></p>
<p>Office: <%= @employee.office %></p>
<img src= <%=@employee.img_url%> alt="employee_image">
<p>Companion Dog: <%= link_to @employee.dog.name, dog_path(@employee.dog)%></p>
<%= button_to "Edit", edit_employee_path(@employee), method: :get %>
